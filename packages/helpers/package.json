{
  "version": "0.5.3",
  "name": "@ratatoskr-ui/helpers",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsdx build --transpileOnly --tsconfig tsconfig.build.json",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepublish": "yarn build",
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose",
    "validate": "yarn lint && yarn type-check",
    "report-coverage": "codecov -f ./coverage/*.json"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "firstaroyand",
  "module": "dist/helpers.esm.js",
  "size-limit": [
    {
      "path": "dist/helpers.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/helpers.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.2",
    "@types/react-dom": "^18.0.0",
    "husky": "^7.0.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@radix-ui/react-portal": "^0.1.4",
    "@radix-ui/react-visually-hidden": "^0.1.4",
    "@ratatoskr-ui/design-token": "^0.5.3",
    "focus-trap": "^6.7.3"
  },
  "gitHead": "706ac39cb27a97de9dba7495d4b7ab1ef5457931"
}
